
public class TimeTable {

	public static void main(String[] args) {
	System.out.println("* |  1  2  3  4  5  6  7  8  9");
	System.out.println("------------------------------");
	for(int i=1;i<=9;i++) {//loop to count row
		System.out.println(i + " |");
		for(int j=1;j<=9;j++){//nested loop to count column
			System.out.print("  "+j+"  ");
			}
		}
	}

}
