import java.util.ArrayList;
import java.util.Iterator;

public class ManyCourses {
	private ArrayList Courses;
	public ManyCourses(){
		this.Courses = new ArrayList();//constructor
		//creating a new ArrayList
	}
	public void addCourse(Learnable c) {
		this.Courses.add(c);c//add a learnable to list
	}
	public void listCourses() {
		for(Learnable c: this.Courses) {
			System.out.println();//print information one by one
		}
	}
}
